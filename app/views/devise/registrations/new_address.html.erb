<h2>住所情報登録!!!</h2>


<%= form_for @address do |f| %>
  <%= render "devise/shared/error_messages", resource: @address %>

 <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :postNo, autofocus: true, autocomplete: "postNo" %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :pref %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :city %>
  </div>

<div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :block %>
  </div>

<div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :building %>
  </div>

<div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :family_name %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :first_name %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :family_name_f %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :first_name_f %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :phon %>
  </div>


  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>