<h2>名前登録</h2>

<%= form_for @name do |f| %>
  <%= render "devise/shared/error_messages", resource: @name %>

 <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :family_name %>
  </div>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :first_name %>
  </div>

<div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :family_name_f %>
  </div>

<div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :first_name_f %>
  </div>

<div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :birthday %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>